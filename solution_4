//2 Points
//Check if the given number is a prime number or not?
function isPrime(numbers){

  for (var i = 2; i < numbers; i++)
                    if (numbers % i === 0)
                        return 'Is Not Prime';
                return numbers > 1 && 'Is prime';

            }

console.log(isPrime(13) ? 'Prime' : 'Is Not a Prime');
